<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}
				Aromielis
			{% endblock %}
		</title>
		{% block stylesheets %}
			<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
		{% endblock %}

		{% block javascripts %}
			{% block importmap %}
				{{ importmap('app') }}
			{% endblock %}
		{% endblock %}
	</head>
	<body>
		<header>
			{% include "shared/_navbar.html.twig" %}
		</header>
		<main role="main">
			{% block body %}
				{% include "shared/_flash.html.twig" %}
			{% endblock %}
			{% include "shared/_footer.html.twig" %}
		</main>
	</body>
</html>
